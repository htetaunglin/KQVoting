<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             xmlns:app="http://schemas.android.com/apk/res-auto"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".Activity.Fragments.ResultFragment">
    <!--<androidx.swiperefreshlayout.widget.SwipeRefreshLayout-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="match_parent"-->
    <!--android:id="@+id/refreshResult">-->
    <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#f4f4f4"
                android:orientation="vertical">
            <!-- top row _ result -->
            <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_marginBottom="7dp"
                    android:background="#fff">
                <!--king-->
                <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/king_icon"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_alignTop="@+id/donut_progress"
                        android:layout_marginLeft="16dp"
                        android:layout_marginTop="8dp"
                        android:src="@drawable/king_icon"
                        tools:ignore="VectorDrawableCompat"/>

                <TextView
                        android:id="@+id/king_percent"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/king_icon"
                        android:layout_alignTop="@+id/king_icon"
                        android:layout_marginLeft="12dp"
                        android:layout_toRightOf="@+id/king_icon"
                        android:gravity="center_vertical"
                        android:text="0%"/>
                <!--Queen-->
                <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/queen_icon"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_alignParentRight="true"
                        android:layout_alignTop="@+id/donut_progress"
                        android:layout_marginRight="16dp"
                        android:layout_marginTop="8dp"
                        android:src="@drawable/crown"
                        tools:ignore="VectorDrawableCompat"/>

                <TextView
                        android:id="@+id/queen_percent"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/queen_icon"
                        android:layout_alignTop="@+id/queen_icon"
                        android:layout_marginRight="12dp"
                        android:layout_toLeftOf="@+id/queen_icon"
                        android:gravity="center_vertical"
                        android:text="0%"/>

                <com.github.lzyzsd.circleprogress.DonutProgress
                        android:id="@+id/donut_progress"
                        android:layout_width="150dp"
                        android:layout_height="150dp"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        app:donut_progress="0"
                        app:donut_show_text="false"/>

                <TextView
                        android:id="@+id/super_progress_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:gravity="center"
                        android:text="0\n/\n0"
                        android:textSize="21sp"/>

                <TextView
                        android:id="@+id/noConnection"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:gravity="center"
                        android:visibility="gone"
                        android:textColor="@android:color/holo_red_dark"
                        android:text="No\nconnection"
                        android:textSize="18sp"/>
                <!-- male -->
                <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/male_icon"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_alignBottom="@+id/donut_progress"
                        android:layout_marginLeft="16dp"
                        android:layout_marginTop="8dp"
                        android:src="@drawable/male_icon"
                        tools:ignore="VectorDrawableCompat"/>

                <TextView
                        android:id="@+id/male_percent"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/male_icon"
                        android:layout_alignTop="@+id/male_icon"
                        android:layout_marginLeft="12dp"
                        android:layout_toRightOf="@+id/male_icon"
                        android:gravity="center_vertical"
                        android:text="0%"/>
                <!--female-->
                <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/female_icon"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_alignBottom="@+id/donut_progress"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="16dp"
                        android:layout_marginTop="8dp"
                        android:src="@drawable/female_icon"
                        tools:ignore="VectorDrawableCompat"/>

                <TextView
                        android:id="@+id/female_percent"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/female_icon"
                        android:layout_alignTop="@+id/female_icon"
                        android:layout_marginRight="12dp"
                        android:layout_toLeftOf="@+id/female_icon"
                        android:gravity="center_vertical"
                        android:text="0%"/>
            </RelativeLayout>

            <RelativeLayout
                    android:id="@+id/liveRtext"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:layout_marginBottom="7dp"
                    android:padding="2dp"
                    android:background="#fff">

                <edu.ucsmub.kqvoting.customUI.UZawTextView
                        android:layout_width="wrap_content"
                        android:layout_centerInParent="true"
                        android:drawableLeft="@drawable/alert"
                        android:gravity="center"
                        android:layout_height="wrap_content"
                        android:text="Live Result ကို ပိတ်ထားပါသည်"
                        android:layout_margin="2dp"
                        android:textColor="@android:color/holo_red_light"/>
            </RelativeLayout>
            <!--king result-->
            <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:layout_marginBottom="7dp"
                    android:background="#fff">

                <de.hdodenhof.circleimageview.CircleImageView
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/king_profile_image"
                        android:layout_width="96dp"
                        android:layout_height="96dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="25dp"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="28dp"
                        android:src="@drawable/pending_people"
                        app:civ_border_color="#f1f1f1"
                        app:civ_border_width="2dp"
                        tools:ignore="VectorDrawableCompat"/>

                <edu.ucsmub.kqvoting.customUI.UZawTextView
                        android:id="@+id/king_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignEnd="@+id/king_profile_image"
                        android:layout_alignLeft="@+id/king_profile_image"
                        android:layout_alignRight="@+id/king_profile_image"
                        android:layout_alignStart="@+id/king_profile_image"
                        android:layout_below="@+id/king_profile_image"
                        android:layout_marginTop="11dp"
                        android:gravity="center"
                        android:text="\?"/>
                <!--King header -->
                <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/king_result_header_icon"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="16dp"
                        android:layout_marginTop="16dp"
                        android:src="@drawable/king_icon"
                        tools:ignore="VectorDrawableCompat"/>

                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/king_result_header_icon"
                        android:layout_alignTop="@+id/king_result_header_icon"
                        android:layout_marginRight="12dp"
                        android:layout_toLeftOf="@+id/king_result_header_icon"
                        android:gravity="center_vertical"
                        android:text="King"
                        android:textColor="@color/colorPrimary"
                        android:textSize="18sp"/>
                <!--progress-->
                <TextView
                        android:id="@+id/king_progress_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_above="@+id/king_progressBar"
                        android:layout_alignEnd="@+id/king_progressBar"
                        android:layout_alignRight="@+id/king_progressBar"
                        android:text="0/0"/>

                <ProgressBar
                        android:id="@+id/king_progressBar"
                        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:layout_alignEnd="@+id/king_result_header_icon"
                        android:layout_alignRight="@+id/king_result_header_icon"
                        android:layout_alignTop="@+id/king_name"
                        android:max="0"
                        android:progress="0"/>
            </RelativeLayout>
            <!--queen result-->
            <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:layout_marginBottom="7dp"
                    android:background="#fff">

                <de.hdodenhof.circleimageview.CircleImageView
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/queen_profile_image"
                        android:layout_width="96dp"
                        android:layout_height="96dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="25dp"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="28dp"
                        android:src="@drawable/pending_people"
                        app:civ_border_color="#f1f1f1"
                        app:civ_border_width="2dp"
                        tools:ignore="VectorDrawableCompat"/>

                <edu.ucsmub.kqvoting.customUI.UZawTextView
                        android:id="@+id/queen_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignEnd="@+id/queen_profile_image"
                        android:layout_alignLeft="@+id/queen_profile_image"
                        android:layout_alignRight="@+id/queen_profile_image"
                        android:layout_alignStart="@+id/queen_profile_image"
                        android:layout_below="@+id/queen_profile_image"
                        android:layout_marginTop="11dp"
                        android:gravity="center"
                        android:text="\?"/>
                <!--King header -->
                <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/queen_result_header_icon"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="16dp"
                        android:layout_marginTop="16dp"
                        android:src="@drawable/crown"
                        tools:ignore="VectorDrawableCompat"/>

                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/queen_result_header_icon"
                        android:layout_alignTop="@+id/queen_result_header_icon"
                        android:layout_marginRight="12dp"
                        android:layout_toLeftOf="@+id/queen_result_header_icon"
                        android:gravity="center_vertical"
                        android:text="Queen"
                        android:textColor="@color/colorPrimary"
                        android:textSize="18sp"/>
                <!--progress-->
                <TextView
                        android:id="@+id/queen_progress_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_above="@+id/queen_progressBar"
                        android:layout_alignEnd="@+id/queen_progressBar"
                        android:layout_alignRight="@+id/queen_progressBar"
                        android:text="0/0"/>

                <ProgressBar
                        android:id="@+id/queen_progressBar"
                        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:layout_alignEnd="@+id/queen_result_header_icon"
                        android:layout_alignRight="@+id/queen_result_header_icon"
                        android:layout_alignTop="@+id/queen_name"
                        android:max="0"
                        android:progress="0"/>
            </RelativeLayout>
            <!--male result-->
            <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:layout_marginBottom="7dp"
                    android:background="#fff">

                <de.hdodenhof.circleimageview.CircleImageView
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/male_profile_image"
                        android:layout_width="96dp"
                        android:layout_height="96dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="25dp"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="28dp"
                        android:src="@drawable/pending_people"
                        app:civ_border_color="#f1f1f1"
                        app:civ_border_width="2dp"
                        tools:ignore="VectorDrawableCompat"/>

                <edu.ucsmub.kqvoting.customUI.UZawTextView
                        android:id="@+id/popular_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignEnd="@+id/male_profile_image"
                        android:layout_alignLeft="@+id/male_profile_image"
                        android:layout_alignRight="@+id/male_profile_image"
                        android:layout_alignStart="@+id/male_profile_image"
                        android:layout_below="@+id/male_profile_image"
                        android:layout_marginTop="11dp"
                        android:gravity="center"
                        android:text="\?"/>
                <!--King header -->
                <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/male_result_header_icon"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="16dp"
                        android:layout_marginTop="16dp"
                        android:src="@drawable/male_icon"
                        tools:ignore="VectorDrawableCompat"/>

                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/male_result_header_icon"
                        android:layout_alignTop="@+id/male_result_header_icon"
                        android:layout_marginRight="12dp"
                        android:layout_toLeftOf="@+id/male_result_header_icon"
                        android:gravity="center_vertical"
                        android:text="Mr Popular"
                        android:textColor="@color/colorPrimary"
                        android:textSize="18sp"/>
                <!--progress-->
                <TextView
                        android:id="@+id/popular_progress_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_above="@+id/male_progressBar"
                        android:layout_alignEnd="@+id/male_progressBar"
                        android:layout_alignRight="@+id/male_progressBar"
                        android:text="0/0"/>

                <ProgressBar
                        android:id="@+id/male_progressBar"
                        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:layout_alignEnd="@+id/male_result_header_icon"
                        android:layout_alignRight="@+id/male_result_header_icon"
                        android:layout_alignTop="@+id/popular_name"
                        android:max="0"
                        android:progress="0"/>
            </RelativeLayout>
            <!--female result-->
            <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:layout_marginBottom="7dp"
                    android:background="#fff">

                <de.hdodenhof.circleimageview.CircleImageView
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:id="@+id/female_profile_image"
                        android:layout_width="96dp"
                        android:layout_height="96dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="25dp"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="28dp"
                        android:src="@drawable/pending_people"
                        app:civ_border_color="#f1f1f1"
                        app:civ_border_width="2dp"
                        tools:ignore="VectorDrawableCompat"/>

                <edu.ucsmub.kqvoting.customUI.UZawTextView
                        android:id="@+id/innocence_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignEnd="@+id/female_profile_image"
                        android:layout_alignLeft="@+id/female_profile_image"
                        android:layout_alignRight="@+id/female_profile_image"
                        android:layout_alignStart="@+id/female_profile_image"
                        android:layout_below="@+id/female_profile_image"
                        android:layout_marginTop="11dp"
                        android:gravity="center"
                        android:text="\?"/>
                <!--King header -->
                <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/female_result_header_icon"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="16dp"
                        android:layout_marginTop="16dp"
                        android:src="@drawable/female_icon"
                        tools:ignore="VectorDrawableCompat"/>

                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/female_result_header_icon"
                        android:layout_alignTop="@+id/female_result_header_icon"
                        android:layout_marginRight="12dp"
                        android:layout_toLeftOf="@+id/female_result_header_icon"
                        android:gravity="center_vertical"
                        android:text="Miss Innocence"
                        android:textColor="@color/colorPrimary"
                        android:textSize="18sp"/>
                <!--progress-->
                <TextView
                        android:id="@+id/innocence_progress_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_above="@+id/female_progressBar"
                        android:layout_alignEnd="@+id/female_progressBar"
                        android:layout_alignRight="@+id/female_progressBar"
                        android:text="0/0"/>

                <ProgressBar
                        android:id="@+id/female_progressBar"
                        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:layout_alignEnd="@+id/female_result_header_icon"
                        android:layout_alignRight="@+id/female_result_header_icon"
                        android:layout_alignTop="@+id/innocence_name"
                        android:max="0"
                        android:progress="0"/>
            </RelativeLayout>

        </LinearLayout>
    </androidx.core.widget.NestedScrollView>

    <!--</androidx.swiperefreshlayout.widget.SwipeRefreshLayout>-->

</FrameLayout>